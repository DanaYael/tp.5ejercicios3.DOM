{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/app.js" // Asegúrate de que este sea el archivo principal de tu proyecto
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:5501", // Cambia este valor según tu puerto de Live Server
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}
